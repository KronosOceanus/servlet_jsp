<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>ScopedVars</servlet-name>
        <servlet-class>el.ScopedVars</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ScopedVars</servlet-name>
        <url-pattern>/el.ScopedVars</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>BeanProperties</servlet-name>
        <servlet-class>el.BeanProperties</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BeanProperties</servlet-name>
        <url-pattern>/el.BeanProperties</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>Collectionss</servlet-name>
        <servlet-class>el.Collectionss</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Collectionss</servlet-name>
        <url-pattern>/el.Collectionss</url-pattern>
    </servlet-mapping>
</web-app>