<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>RandomNumber</servlet-name>
        <servlet-class>jsp_java.RandomNumber</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RandomNumber</servlet-name>
        <url-pattern>/jsp_java.RandomNumber</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SomeHeading</servlet-name>
        <servlet-class>jsp_java.SomeHeading</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SomeHeading</servlet-name>
        <url-pattern>/jsp_java.SomeHeading</url-pattern>
    </servlet-mapping>

</web-app>