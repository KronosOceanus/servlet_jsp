<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>RepeatVisitor</servlet-name>
        <servlet-class>cookie.RepeatVisitor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RepeatVisitor</servlet-name>
        <url-pattern>/cookie.RepeatVisitor</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CookieTest</servlet-name>
        <servlet-class>cookie.CookieTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CookieTest</servlet-name>
        <url-pattern>/cookie.CookieTest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RepeatVisitor2</servlet-name>
        <servlet-class>cookie.RepeatVisitor2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RepeatVisitor2</servlet-name>
        <url-pattern>/cookie.RepeatVisitor2</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ClientAccessCounts</servlet-name>
        <servlet-class>cookie.ClientAccessCounts</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientAccessCounts</servlet-name>
        <url-pattern>/cookie.ClientAccessCounts</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>cookie.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/cookie.RegistrationServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistrationForm</servlet-name>
        <servlet-class>cookie.RegistrationForm</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationForm</servlet-name>
        <url-pattern>/cookie.RegistrationForm</url-pattern>
    </servlet-mapping>

</web-app>